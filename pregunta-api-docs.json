  "components": {
    "schemas": {
      "SubCategoriaDTO": {
        "required": [
          "name",
          "status",
          "subCategoryId"
        ],
        "type": "object",
        "properties": {
          "subCategoryId": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "maxLength": 60,
            "minLength": 0,
            "type": "string"
          },
          "status": {
            "type": "boolean"
          }
        }
      },
      "GeneralResponseSubCategoriaDTO": {
        "type": "object",
        "properties": {
          "error": {
            "type": "boolean"
          },
          "descripcionRespuesta": {
            "type": "string"
          },
          "resultado": {
            "$ref": "#/components/schemas/SubCategoriaDTO"
          }
        }
      },
      "PreguntaDTO": {
        "type": "object",
        "properties": {
          "questionId": {
            "type": "string",
            "format": "uuid"
          },
          "questionTypeId": {
            "type": "string",
            "format": "uuid"
          },
          "categoryId": {
            "type": "string",
            "format": "uuid"
          },
          "text": {
            "type": "string"
          },
          "status": {
            "type": "boolean"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "createdBy": {
            "type": "string",
            "format": "uuid"
          },
          "updatedAt": {
            "type": "string",
            "format": "date-time"
          },
          "updatedBy": {
            "type": "string",
            "format": "uuid"
          },
          "videoUrl": {
            "type": "string"
          },
          "image": {
            "type": "string"
          },
          "order": {
            "type": "integer",
            "format": "int32"
          },
          "note": {
            "type": "string"
          },
          "isRequired": {
            "type": "boolean"
          },
          "videoAltText": {
            "type": "string"
          },
          "imageAltText": {
            "type": "string"
          }
        }
      },
      "GeneralResponsePreguntaDTO": {
        "type": "object",
        "properties": {
          "error": {
            "type": "boolean"
          },
          "descripcionRespuesta": {
            "type": "string"
          },
          "resultado": {
            "$ref": "#/components/schemas/PreguntaDTO"
          }
        }
      },
      "TipoPreguntaDTO": {
        "required": [
          "name",
          "questionTypeId",
          "status"
        ],
        "type": "object",
        "properties": {
          "questionTypeId": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "maxLength": 60,
            "minLength": 0,
            "type": "string"
          },
          "status": {
            "type": "boolean"
          }
        }
      },
      "GeneralResponseTipoPreguntaDTO": {
        "type": "object",
        "properties": {
          "error": {
            "type": "boolean"
          },
          "descripcionRespuesta": {
            "type": "string"
          },
          "resultado": {
            "$ref": "#/components/schemas/TipoPreguntaDTO"
          }
        }
      },
      "CategoriaDTO": {
        "required": [
          "categoryId",
          "name",
          "status"
        ],
        "type": "object",
        "properties": {
          "categoryId": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "maxLength": 60,
            "minLength": 0,
            "type": "string"
          },
          "status": {
            "type": "boolean"
          },
          "subCategories": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/SubCategoriaDTO"
            }
          }
        }
      },
      "GeneralResponseCategoriaDTO": {
        "type": "object",
        "properties": {
          "error": {
            "type": "boolean"
          },
          "descripcionRespuesta": {
            "type": "string"
          },
          "resultado": {
            "$ref": "#/components/schemas/CategoriaDTO"
          }
        }
      },
      "AsociarPreguntaSeccionDTO": {
        "required": [
          "questionId",
          "sectionId"
        ],
        "type": "object",
        "properties": {
          "questionId": {
            "type": "string",
            "format": "uuid"
          },
          "sectionId": {
            "type": "string",
            "format": "uuid"
          }
        }
      },
      "GeneralResponseVoid": {
        "type": "object",
        "properties": {
          "error": {
            "type": "boolean"
          },
          "descripcionRespuesta": {
            "type": "string"
          },
          "resultado": {
            "type": "object"
          }
        }
      },
      "ClonarPreguntaDTO": {
        "type": "object",
        "properties": {
          "questionId": {
            "type": "string",
            "format": "uuid"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
